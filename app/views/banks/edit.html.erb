<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "Alterar Banco #{@bank.name}", "#" %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"></h2>
    <div class="inner">
      <% form_for @bank, :url => bank_path(@bank), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>

